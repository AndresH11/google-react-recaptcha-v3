{
  "name": "google-react-recaptcha-v3",
  "version": "2.0.0",
  "description": "React component for Google reCAPTCHA v3 with advanced features and optimizations",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "prepublish": "npm run build",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit",
    "examples": "npm run build && cd examples && npm run dev",
    "examples:build": "npm run build && cd examples && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AndresH11/google-react-recaptcha-v3.git"
  },
  "bugs": {
    "url": "https://github.com/AndresH11/google-react-recaptcha-v3/issues"
  },
  "homepage": "https://github.com/AndresH11/google-react-recaptcha-v3/blob/main/README.md",
  "keywords": [
    "react",
    "recaptcha",
    "recaptcha-v3",
    "google-recaptcha",
    "typescript",
    "react-component",
    "form-validation",
    "bot-protection",
    "trusted-types",
    "performance",
    "optimized",
    "csp",
    "security"
  ],
  "author": {
    "name": "Andres Felipe Hernandez Aldana",
    "url": "https://github.com/AndresH11"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  }
}
